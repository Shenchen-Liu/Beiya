	padding:0 auto;
	width:90px;
	text-align:center;
	line-height:50px;
}
#navigation{
	background: rgb(255,171,17);
	height:60px;
	margin: 0 auto;
	padding: 0 auto;
	width:980px;
}

#navigation #Info{
	font: 24px/1.5 "微软雅黑",arial,helvetica,clean,sans-serif;
	font-weight:bolder;
	float:left;
	text-align:center;
}
#Info li{
	width:80px;
	float:left;
	  position:relative;
}
#InfoBg{
		background-image:url(../img/category.png)
}
#Info ul ul{
	width:500px;
	padding-top:-10px;
	font-size:24px;
	z-index:-1;
	position:relative;
	display:none;
}
#Info li{
	line-height:60px;
}
.hover{
	display:block;
}
.InfoDetail li{
	margin-top:10px;
  float: left;
  font-family: 微软雅黑,Arial;
  font-size: 24px;
  line-height: 50px;
  display:block;
  height:48px;
}
.logo2{
	float:left;
}
.search{
	margin: 0 auto;
	margin: 0 20px 0 0;
	margin-top:15px;
	background:#FFF;
	float:right;
	
}
.search2 {
	float:left;
	height:35px;
}
.search input{
	background: url(../img/searchSign.png) no-repeat scroll 3px -1px transparent;
	margin: 0 auto;
	padding: 0;
	border: 2px solid rgb(186,186,186);
    color: #BABABA;
    float: left;
    font: 12px/31px arial;
    height: 35px;
    outline: medium none;
    padding-left: 36px;
    vertical-align: top;
    width: 210px;
}
.search-btn{
    background: url("../img/searchBtn.png") no-repeat scroll 0 0 transparent;
    border: medium none;
    cursor: pointer;
    display: inline-block;
    height: 35px;
    margin-left: -2px;
    width: 65px;
}
#user{
	height:60px;
	margin: 0 auto;
	width:980px;
	background:rgb(0,168,255);
	font-size:18px;
}

#user_0{
	width:100px;
	height:60px;
	line-height:60px;
	text-align:center;
	float:right;
	transition-duration: 0.7s;
    transition-property: background-color;
    transition-timing-function: ease-out;
}

.nums ul li a {
    color: rgb(102, 102, 102);
    display: block;
    padding: 0 20px;
    text-align: center;
    text-shadow: 1px 1px 1px rgb(238, 238, 238);
}
.nums a {
    display: none;
}
a {
    color: rgb(0, 0, 0);
    cursor: pointer;
    outline: 0 none;
    text-decoration: none;
}
.nums ul li {
    font-family: 微软雅黑,Arial;
    font-size: 12px;
    line-height: 16px;
}
h1, h2, h3, h4, h5, h6, ul, li {
    list-style-type: none;
}
#main{
